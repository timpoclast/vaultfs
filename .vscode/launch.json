	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Module",
			"type": "python",
			"request": "launch",
			// "module": "$('mytest.')${file}"
			"module": "vaultfs.vaultfs",
			"console": "integratedTerminal",
			"args": [
				"-m", "/mnt/vaultfs",
				"-l", "${env:HOME}/.local/vaultfs",
				"-r", "https://127.0.0.1:8200",
				"-s", "users",
				"-p", "${env:HOME}/.vault-token"
			]
		}
	]
}